{
    "compilerOptions": {
        "outDir": "./build/",
        "noImplicitAny": true,
        "sourceMap": true,
        "target": "es5",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,    
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
    },
    "exclude": [
        "node_modules",
        "dist",
        "webpack.config.js",
        "src",
        "build",
    ]
}