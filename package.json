{
  "type": "module",
  "sideEffects": true,
  "scripts": {
    "build:webpack": "tsc -p tsconfig.webpack.json",
    "build:server": "tsc -p tsconfig.server.json",
    "watch:webpackconfig": "tsc -w -p tsconfig.webpack.json",
    "watch:serverconfig": "tsc -w -p tsconfig.server.json",
    "watch:dev:webpack": "webpack --watch --config build/webpack.dev.config.js",
    "watch:prod:webpack": "webpack --watch --config build/webpack.prod.config.js",
    "dev:watch": "npm-run-all --parallel watch:webpackconfig watch:serverconfig watch:dev:webpack",
    "prod:watch": "npm-run-all --parallel watch:webpackconfig watch:serverconfig watch:prod:webpack",
    "dev:build": "npm-run-all --parallel build:webpack build:server",
    "prod:build": "npm-run-all --parallel build:webpack build:server",
    "run:dev": "sleep 2.5 && cross-env NODE_ENV=development nodemon build/server/server.js",
    "run:prod": "sleep 5 && cross-env NODE_ENV=production nodemon build/server/server.js",
    "dev": "npm run dev:build && npm-run-all --parallel dev:watch run:dev",
    "prod": "npm run prod:build && npm-run-all --parallel prod:watch run:prod"
  },
  "dependencies": {
