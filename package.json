{
  "type": "module",
  "scripts": {
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "webpack --config build/server/webpack.config.js",
    "build": "npm run build:server && npm run build:client",
    "dev:server": "tsc -w -p tsconfig.server.json",
    "dev:client": "webpack --watch --config build/server/webpack.config.js",
    "dev:init": "npm run build:server && npm run dev",
    "watch:client": "nodemon --watch build/server/webpack.config.js --exec \"npm run dev:client\"",
    "dev": "concurrently \"npm:dev:server\" \"npm:watch:client\" \"nodemon build/server/server.js --watch build/server\"",
    "start": "npm run build && node build/server/server.js",
    "deploy": "npm run start"
  },
  "dependencies": {
    "@blockly/continuous-toolbox": "^7.0.0",
    "@blockly/field-colour": "^6.0.0",
    "@blockly/theme-modern": "^7.0.0",
    "@stripe/stripe-js": "^7.3.1",
    "blockly": "^12.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "eta": "^3.5.0",
    "express": "^4.21.2",
    "html-bundler-webpack-plugin": "^4.19.1",
    "memory-cache": "^0.2.0",
    "parse5": "^7.2.1",
    "prismjs": "^1.30.0",
    "rehype-document": "^7.0.3",
    "rehype-format": "^5.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "relative-time": "^1.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-sectionize": "^2.1.0",
    "sass-embedded": "^1.86.0",
    "stripe": "^17.7.0",
    "style-loader": "^4.0.0",
    "tsx": "^4.20.2",
    "unified": "^11.0.5",
    "unist-util-find-after": "^5.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@blockly/dev-tools": "^9.0.1",
    "@types/express": "^5.0.1",
    "@types/gtag.js": "^0.0.20",
    "@types/mdast": "^4.0.4",
    "@types/memory-cache": "^0.2.6",
    "@types/node": "^22.15.31",
    "@types/w3c-web-serial": "^1.0.8",
    "@types/w3c-web-usb": "^1.0.10",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.1",
    "@types/webpack-env": "^1.18.8",
    "@types/wicg-file-system-access": "^2023.10.6",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.1",
    "markdown-loader": "^8.0.0",
    "nodemon": "^3.1.10",
    "responsive-loader": "^3.1.2",
    "rimraf": "^6.0.1",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "sharp": "^0.34.2",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  }
