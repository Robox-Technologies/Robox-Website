{
  "type": "module",
  "scripts": {
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:analyze": "npm run clean && tsc -p tsconfig.webpack.json && cross-env NODE_ENV=production ANALYZE=true webpack --config build/webpack.prod.config.js",
    "clean": "rimraf build",
    "dev:server": "tsc -w -p tsconfig.server.json",
    "dev:run": "cross-env NODE_ENV=development nodemon build/server/server.js --watch build/server",
    "dev": "npm run clean && tsc -p tsconfig.webpack.json && npm-run-all --parallel dev:server dev:run"
  },
